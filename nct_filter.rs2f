type: enumlist
label: Herbs to always hide regardless of value
enum: [
  "guam leaf",
  "marrentill",
  "tarromin",
  "harralander",
  "ranarr weed",
  "toadflax",
  "irit leaf",
  "avantoe",
  "kwuarm",
  "huasca",
  "snapdragon",
  "cadantine",
  "lantadyme",
  "dwarf weed",
  "torstol"
]
*/
